{
    "name": "lionix/envclient",
    "description": "Laravel environment client and console commands.",
    "keywords": ["laravel", "env", "dotenv", "environment", "validation", "lionix"],
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "laravel/framework": "^6.0"
    },    
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Lionix\\EnvClient\\": "src/app"
        }
    },    
    "autoload-dev": {
        "psr-4": {
            "Lionix\\EnvClient\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lionix\\EnvClient\\Providers\\EnvClientServiceProvider"
            ],
            "aliases": {
                "Lionix\\EnvClient": "Lionix\\EnvClient\\Services\\EnvClient",
                "Lionix\\EnvGetter": "Lionix\\EnvClient\\Services\\EnvGetter",
                "Lionix\\EnvSetter": "Lionix\\EnvClient\\Services\\EnvSetter",
                "Lionix\\EnvValidator": "Lionix\\EnvClient\\Services\\EnvValidator"
            }
        }
    },
    "authors": [
        {
            "name": "Lionix Team",
            "email": "packages@lionix-team.com"
        },
        {
            "name": "Anastas Vartanyan",
            "email": "vaawebdev@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
